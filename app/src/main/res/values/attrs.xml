<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--A variant of primarySurface which corresponds to colorPrimaryVariant in light theme-->
    <!--and colorSurface in dark theme-->
    <attr name="colorPrimarySurfaceVariant" format="color" />

    <!--Emphasis values are used in combination with each "on" color to create three available -->
    <!--levels of emphasis for text and content.-->
    <attr name="emphasisHighAlpha" format="float" />
    <attr name="emphasisMediumAlpha" format="float" />
    <attr name="emphasisDisabledAlpha" format="float" />

    <attr name="motionInterpolatorPersistent" format="reference" />
    <attr name="motionInterpolatorIncoming" format="reference" />
    <attr name="motionInterpolatorOutgoing" format="reference" />

    <!--Custom view themes/styles-->
    <attr name="bottomNavigationDrawerStyle" format="reference" />
    <attr name="bottomNavigationDrawerNavigationViewTheme" format="reference" />

    <attr name="transitionShapeAppearance" format="reference" />
    <attr name="expandedColor" format="color" />

    <declare-styleable name="CreateStepView">
        <attr name="stepColorFinished" format="color" />
        <attr name="stepTextColorFinished" format="color" />
        <attr name="stepColorUnfinished" format="color" />
        <attr name="stepTextColorUnfinished" format="color" />
        <attr name="stepState" format="enum">
            <enum name="unfinished" value="0" />
            <enum name="finished" value="1" />
        </attr>
        <attr name="stepNumberInt" format="integer" />
    </declare-styleable>

    <declare-styleable name="StepView">
        <attr name="svStatus" format="enum">
            <enum name="unfinished" value="0" />
            <enum name="finished" value="1" />
            <enum name="executing" value="2" />
        </attr>

        <attr name="svAnimationDuration" format="integer" />

        <attr name="svLineType" format="enum">
            <enum name="solid" value="0" />
            <enum name="dotted" value="1" />
        </attr>

        <attr name="svCircleRadius" format="dimension" />

        <attr name="svSelectedRadius" format="dimension" />

        <attr name="svFinishedCircleColor" format="color" />
        <attr name="svFinishedTextColor" format="color" />
        <attr name="svFinishedMarkColor" format="color" />
        <attr name="svFinishedLineColor" format="color" />

        <attr name="svUnfinishedCircleColor" format="color" />
        <attr name="svUnfinishedTextColor" format="color" />
        <attr name="svUnfinishedLineColor" format="color" />

        <attr name="svExecutingCircleColor" format="color" />
        <attr name="svExecutingTextColor" format="color" />
        <attr name="svExecutingMarkColor" format="color" />
        <attr name="svExecutingLineColor" format="color" />

        <attr name="svStepNumber" format="integer" />
        <attr name="svStepNumberTextSize" format="dimension" />

        <attr name="svLineLength" format="dimension" />
        <attr name="svLineHeight" format="dimension" />
        <attr name="svLineMargin" format="dimension" />
        <attr name="svSelected" format="boolean" />
        <attr name="svSelectedColor" format="color" />
        <attr name="svSelectedStrokeWidth" format="dimension" />

    </declare-styleable>

    <declare-styleable name="ExpandableLayout">
        <attr name="el_duration" format="integer" />
        <attr name="el_expanded" format="boolean" />
        <attr name="el_parallax" format="integer" />
        <attr name="el_withAnimator" format="boolean" />
    </declare-styleable>

    <declare-styleable name="HorizontalHourView">
        <attr name="hhvTimeType" format="enum">
            <enum name="twelve" value="0" />
            <enum name="twenty_four" value="1" />
        </attr>
        <attr name="hhvSelectedTextColor" format="color" />
        <attr name="hhvUnSelectedColor" format="color" />
        <attr name="hhvSelectedTextSize" format="dimension" />
        <attr name="hhvUnSelectedTextSize" format="dimension" />
        <attr name="hhvNeedParallax" format="boolean" />
        <attr name="hhvArrowRes" format="reference" />
    </declare-styleable>

    <declare-styleable name="WheelView">
        <!-- 中间分割线外的item数量,整个滑动数量就为 wheelItemCount * 2 + 1  -->
        <attr name="wheelItemCount" format="integer" />
        <!-- 滑轮item高度 -->
        <attr name="wheelItemSize" format="dimension" />
        <!-- 分割线颜色 -->
        <attr name="dividerColor" format="color" />
        <!-- 布局方向 -->
        <attr name="wheelOrientation">
            <enum name="vertical" value="1" />
            <enum name="horizontal" value="0" />
        </attr>
        <!-- 水平布局时不考虑只参数,  当垂直布局时的左右靠齐立体效果 -->
        <attr name="wheelGravity">
            <enum name="left" value="1" />
            <enum name="center" value="2" />
            <enum name="right" value="3" />
        </attr>

        <!-- 两根分割线的距离 -->
        <attr name="wheelDividerSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="HorizontalStepView">
        <attr name="hsvStepCount" format="integer" />
        <attr name="hsvCurrentCount" format="integer" />
        <attr name="hsvFinishedDrawable" format="reference" />
        <attr name="hsvExecutingDrawable" format="reference" />
        <attr name="hsvCircleRadius" format="dimension" />
        <attr name="hsvCircleStokeWidth" format="dimension" />
        <attr name="hsvStokeCircleGap" format="dimension" />
        <attr name="hsvCircleFillColor" format="color" />
        <attr name="hsvCircleFillFinishedColor" format="color" />
        <attr name="hsvCircleFillExecutingColor" format="color" />
        <attr name="hsvCircleStokeColor" format="color" />
        <attr name="hsvCircleStokeFinishedColor" format="color" />
        <attr name="hsvCircleStokeExecutingColor" format="color" />
        <attr name="hsvLineLength" format="dimension" />
        <attr name="hsvLineHeight" format="dimension" />
        <attr name="hsvLineGap" format="dimension" />
        <attr name="hsvLineColor" format="color" />
        <attr name="hsvLineFinishedColor" format="color" />
        <attr name="hsvLineExecutingColor" format="color" />
        <attr name="hsvTextColor" format="color" />
        <attr name="hsvTextSize" format="dimension" />
        <attr name="hsvStyle">
            <flag name="fill" value="0" />
            <flag name="fillStoke" value="1" />
        </attr>
        <attr name="hsvSelectedColor" format="color" />
        <attr name="hsvSelectedIndex" format="integer" />
        <attr name="hsvSelectedStrokeWidth" format="dimension" />
        <attr name="hsvSelectedRadius" format="dimension" />
        <attr name="hsvSelectedStyle">
            <flag name="solid" value="0" />
            <flag name="dotted" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="ChoiceInputView">
        <attr name="civIcon" format="reference" />
        <attr name="civTitle" format="string" />
    </declare-styleable>

    <attr name="stepColorFill" format="color" />
    <attr name="stepColorStroke" format="color" />
    <attr name="stepColorFinishedFill" format="color" />
    <attr name="stepColorFinishedStroke" format="color" />
    <attr name="stepColorExecutingFill" format="color" />
    <attr name="stepColorExecutingStroke" format="color" />
    <attr name="stepLineColor" format="color" />
    <attr name="stepLineFinishedColor" format="color" />
    <attr name="stepLineExecutingColor" format="color" />

    <attr name="colorTransparent" format="color" />

    <attr name="tlColor" format="color" />
    <attr name="tlUnfinishedFillColor" format="color" />
    <attr name="tlUnfinishedStrokeColor" format="color" />
    <attr name="tlFinishedFillColor" format="color" />
    <attr name="tlFinishedStrokeColor" format="color" />
    <attr name="tlExpiredFillColor" format="color" />
    <attr name="tlExpiredStrokeColor" format="color" />
    <attr name="tlUnfinishedTextColor" format="color" />
    <attr name="tlFinishedTextColor" format="color" />
    <attr name="tlExpiredTextColor" format="color" />
    <attr name="taskRecyclerBackColor" format="color" />

    <declare-styleable name="TaskTimeLineRecyclerView">
        <attr name="timeLineColor" format="color" />
        <attr name="timeLineUnfinishedFillColor" format="color" />
        <attr name="timeLineUnfinishedStrokeColor" format="color" />
        <attr name="timeLineFinishedFillColor" format="color" />
        <attr name="timeLineFinishedStrokeColor" format="color" />
        <attr name="timeLineExpiredFillColor" format="color" />
        <attr name="timeLineExpiredStrokeColor" format="color" />
        <attr name="timeLineFinishedTextColor" format="color" />
        <attr name="timeLineExpiredTextColor" format="color" />
        <attr name="timeLineUnfinishedTextColor" format="color" />
        <attr name="timeLineTextSize" format="dimension" />
        <attr name="timeLineCircleRadius" format="dimension" />
        <attr name="timeLineWidth" format="dimension" />
        <attr name="timeLineUnfinishedIcon" format="reference" />
        <attr name="timeLineFinishedIcon" format="reference" />
        <attr name="timeLineExpiredIcon" format="reference" />
        <attr name="timeLineStrokeWidth" format="dimension" />
        <attr name="timeLineStartOffset" format="dimension" />
        <attr name="timeLineTopOffset" format="dimension" />
        <attr name="timeLineStyle">
            <flag name="fill" value="0" />
            <flag name="fill_stroke" value="1" />
        </attr>
    </declare-styleable>
</resources>